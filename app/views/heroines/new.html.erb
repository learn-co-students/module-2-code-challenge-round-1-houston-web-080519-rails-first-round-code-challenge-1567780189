<form method="POST" action="/heroines">
<h1> Heroines Form<h1>
<div>
    <label>Name</label>
    <input type="text" name="name"><br>
    <label>Super Name</label>
    <input type="text" name="super_name"><br>
    <label>Power</label>
    <select name="power_id">
    <%Power.all.each do |power|%>
    <option value= "<%= power.id %>">
    <%=power.name%>
    <%end%><br>
</div>
    <input type="submit">