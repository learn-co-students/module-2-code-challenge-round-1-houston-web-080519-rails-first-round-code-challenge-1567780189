<%= form_with({model: Heroine.new}) do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name, required: true %>
    <%= form.label :super_name %>
    <%= form.text_field :super_name, required: true %>
    <%= form.label :super_id %>
    <%= form.collection_select(:super_id, Power.all, :id, :name, prompt: true) %>
    <%= form.submit %>
<%end%>